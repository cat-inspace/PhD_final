Maybe this can be shown with the development of dry matter ofer concentration at the interface after resolubilization.},
author = {Schulte, Jann and St{\"{o}}ckermann, Manuel and Thill, Sebastian and Gebhardt, Ronald},
doi = {10.1016/j.idairyj.2020.104692},
file = {:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/Schulte et al. - 2020 - Calcium effect on the swelling behaviour and stability of casein microparticles.pdf:pdf},
issn = {09586946},
journal = {International Dairy Journal},
title = {{Calcium effect on the swelling behaviour and stability of casein microparticles}},
volume = {105},
year = {2020}
}
@article{Noronha2008,
abstract = {In this study the impact of cooker design and/or blade/auger speed on the functional properties of imitation cheese was investigated. Cheeses with similar compositions (48{\%} moisture) were manufactured using water, rennet casein, rapeseed and hydrogenated palm oil and emulsifying salts in a twin-screw (Blentech) or a single-blade cooker (Stephan). The cookers were operated at auger speeds of 750, 1,125 or 1,500 (Stephan) or 100 or 200 rpm (Blentech). Increasing the blade speed of the Stephan increased (P {\textless} 0.001) the hardness and decreased the meltability of imitation cheese from 112 to 66 mm respectively. Hardness and meltability values of cheeses made in the Blentech cooker were only slightly affected by auger speed. The cheese made in the Stephan at 1,500 rpm had the highest G' and G'' values at 25 degrees C of all the cheeses. This study shows that imitation cheese with acceptable functional properties can be successfully manufactured in a single-blade cooker operated at moderate speeds.},
author = {Noronha, Nessa and O'Riordan, E. Dolores and O'Sullivan, Michael},
doi = {10.1007/s00217-006-0549-9},
file = {:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/Noronha, O'Riordan, O'Sullivan - 2008 - Influence of processing parameters on the texture and microstructure of imitation cheese.pdf:pdf},
isbn = {0021700605},
issn = {14382377},
journal = {European Food Research and Technology},
keywords = {Hardness,Imitation cheese,LM,Processing,Rheology,SEM,Shear rate},
number = {3},
pages = {385--393},
title = {{Influence of processing parameters on the texture and microstructure of imitation cheese}},
volume = {226},
year = {2008}
}
@article{Awad2002,
abstract = {Block-processed Ras cheese was produced with two salt mixtures: (1) Na-diphosphate +Na-polyphosphate + Na-tripolyphosphate in ratios 40:50:10, 30:40:30 {\&} 30:30:40 and (2) Na-polyphosphate + Na-citrate + Na-orthophosphate + Na-diphosphate in ratios 50:20:20: 10, 40:10:20:30 {\&} 30:10:20:40. Commercial salts Joha SE and PZO were used for comparison, respectively. Texture profile analysis and microstructure (LM and TEM) of resultant cheeses were studied. Chewiness, gumminess, adhesiveness, and hardness showed a significant difference and the cheese samples exhibited higher values during storage. The values were highest in the samples stored at room temperature. Light microscopy photographs indicated different emulsification degree with various emulsifying salt mixtures. These observations were confirmed with the image analysis and TEM. Among the entire treatments, mixture (1) in ratio 30:40:30 and mixture (2) in ratio 40:10:20:30 gave the best and close texture to the commercial salts and can be recommended in the production of block processed cheese. {\textcopyright} 2002 Elsevier Science Ltd.},
