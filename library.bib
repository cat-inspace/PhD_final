file = {:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/Leonil et al. - 2008 - Kinetics of Fibril Formation of Bovine -Casein Indicate a Conformational Rearrangement as a Critical Step in the.pdf:pdf;:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/Leonil et al. - 2008 - Kinetics of Fibril Formation of Bovine $\kappa$-Casein Indicate a Conformational Rearrangement as a Critical Step in th.pdf:pdf},
isbn = {0022-2836},
