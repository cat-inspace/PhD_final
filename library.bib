@article{Matsumura1993,
abstract = {Filler effects of oil droplets on the viscoelastic properties of emulsion gels were investigated by small deformation mechanical measurements. The emulsions were made with soya oil and soy 11S globulin (15 wt{\%} oil, 1.5–7{\%} protein concentrations). The emulsion and soy 11S globulin solutions were gelled using Ca++-independent transglutaminase from a microorganism. The shear storage modulus (G′) and loss modulus (G″) of the proteinous gels and emulsion gels depended on the protein concentration. The concentration dependence of the modulus was approximated by an exponential function of the form: G ∝ Cn, n = 4 and n = 2 being found in the cases of the proteinous gels and the emulsion gels, respectively. Shear moduli of the emulsion gels were much higher than those of the proteinous gels. Gels made from a fine emulsion (containing smaller oil droplets) exhibited higher G′ and G″ values than corresponding gels from a coarse emulsion. Addition of Tween 20 was found to reduce G′ and G″ of emulsion gels even when the protein matrix was not significantly displaced from the oil—water interface by the surfactant. {\textcopyright} 2009, Elsevier Ltd. All rights reserved. All rights reserved.},
author = {Matsumura, Yasuki and Kang, Il Jun and Sakamoto, Hiroko and Motoki, Masao and Mori, Tomohiko},
doi = {10.1016/S0268-005X(09)80174-4},
file = {:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/Matsumura et al. - 1993 - Filler effects of oil droplets on the viscoelastic properties of emulsion gels.pdf:pdf},
issn = {0268005x},
journal = {Topics in Catalysis},
number = {3},
pages = {227--240},
title = {{Filler effects of oil droplets on the viscoelastic properties of emulsion gels}},
volume = {7},
year = {1993}
}
@article{Rohm2011,
abstract = {Physical properties, including rheological and texture properties, are important quality parameters of foods and are only partially susceptible to instrumental analytical methods. Texture refers to properties of foods that arise from physicochemical characteristics and which can be perceived by the human, mainly by haptic and kinesthetic stimulation. Although arising from an early classification attempt, it is still useful to distinguish between mechanical, geometrical, and mouthfeel characteristics. Each of these characteristics plays a significant but commodity-dependent role with regard to total food quality judgment by the consumer, and all are accessible by different instrumental methods. Food rheology covers methods for the instrumental determination of mechanical properties, which are based on the response of a particular food to stress or strain and its time dependency. A further classification of rheological properties can be done by considering in detail linear or nonlinear behavior toward stress, strain or shear rate responses, time-dependent behavior, or viscoelasticity.},
