@article{Hemar1999,
abstract = {Diffusing wave spectroscopy (DWS) has been used to study the kinetics of ageing processes in n-decane/water emulsions, stabilised by the milk protein $\alpha$(S1)-casein. A particular advantage of this particle size measuring technique is its direct applicability to the concentrated emulsion, avoiding the necessity for dilution to single scattering levels demanded by traditional dynamic light scattering methods. The observed droplet growth rates were found to conform to the kinetic law of Ostwald ripening. Monotonic increases in coarsening rates have been observed with increasing oil volume fraction and these results are compared with theoretical predictions. The effect of excess protein has also been measured but no evidence for depletion flocculation due to free $\alpha$(S1)-casein was detectable.},
