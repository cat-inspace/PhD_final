pages = {715--755},
publisher = {Academic Press},
title = {{Chapter 29 - Ingredient Cheese and Cheese-Based Ingredients}},
url = {http://www.sciencedirect.com/science/article/pii/B9780124170124000296},
year = {2017}
}
