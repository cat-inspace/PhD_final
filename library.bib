doi = {10.1016/j.jfoodeng.2010.10.014},
file = {:Users/catinspace/Desktop/Literatur{\_}02-20/j.jfoodeng.2010.10.014.pdf:pdf},
issn = {02608774},
journal = {Journal of Food Engineering},
