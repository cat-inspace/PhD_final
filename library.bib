@article{Whittall1996,
abstract = {1 INTRODUCTION Extracting exponential relaxation times and amplitudes from an NMR relaxation decay curve requires an inversion of the Laplace transform. Because the Laplace transform describes such a wide variety of physical systems, its ill-conditioned in-version has been, and continues to be, studied extensively. New publications appear frequently. Stoeps (1993) gave an excellent review of numerous approaches, and much insight into the associated problems. This review begins with a description of the governing equations in Section 2. Sec-tions 3 through 8 sort methods of constructing a solution into several general classes including: nonlinear parameter estimation of relaxation times and amplitudes of a few components, linear inverse techniques which find amplitudes on a specified grid of relaxation times, orthogonal function representations of the continuous or dis-crete formulation, linear prediction methods which encode new decay-curve points in terms of known ones, rational function approaches which exploit the form of the transform of a summation of delta-function components, and deconvolution methods which use a change of variables to convert the Laplace transform to a convolution in-tegral. Section 9 describes appraisal techniques which calculate information common to every solution reproducing the data. Various methods for quantifying resolution are also described. Unfortunately, several algorithms are not discussed here. Significant omissions include the Bayesian approach (e.g., Bretthorst, 19909, 1990b, 1990c) and maximum entropy methods (e.g., Livesey et al., 1986; Press et al., 1988). Also missing are the approximate and non-approximate noise-free techniques surveyed by Davies and Martin (1979), and the two-dimensional, joint T1 and T2 inversion of Whittall (1994). Existence, uniqueness, construction, and appraisal are four fundamentals of the analysis of the inverse Laplace transform or any other Fredhohn integral equation of the first kind. These are discussed in turn.},
author = {Whittall, Kenneth P},
file = {:Users/catinspace/Desktop/Literatur{\_}02-20/S0922-3487{\%}2896{\%}2980040-2.pdf:pdf},
journal = {Signal Treatment and Signal Anallysis in NMR},
number = {1979},
title = {{Chapter 3 A N A L Y S I S OF N M R R E L A X A T I O N Department of Radiology University of British Columbia}},
year = {1996}
}
@article{Farrer1999,
abstract = {A detailed study of the concentration dependence of the osmotic pressure of sodium caseinate solutions encompassing the dilute, semi-dilute and highly concentrated regimes is presented. For the semi-dilute regime, assuming the model of sub-micelles forming 'association polymers', the data is considered in relation to the adhesive hard sphere (AHS) model and an adapted f-functional polycondensation theory. The AHS model fits reasonably at higher temperatures but is unsatisfactory at 30°C. The polycondensation theory in general provides better fits and indicates low functionality (f ca. 3) at 30°C suggesting non-centrally symmetric growth of aggregates (not within the scope of the AHS (f = ∞) model), a prediction supported by EM studies. Both models suggest small sub-micellar building blocks of diameter ca. 11 nm and aggregation number 4-5 with implicit voluminosity comparable to that of native casein sub-micelles. To represent the behaviour in the highly concentrated regime, above close packing of the sub-micelles, a rigid lattice model is proposed which suggests 'soft sphere' potentials for the sub-micelles (V(r){\~{}} r-6.75). Continuous shear rheology, contrasted with the Krieger-Dougherty hard sphere prediction, shows the expected sensitivity to aggregation at low phase volumes and to particle softness above close packing.},
author = {Farrer, Donald and Lips, Alex},
doi = {10.1016/S0958-6946(99)00075-8},
file = {:Users/catinspace/Desktop/Literatur{\_}02-20/s0958-6946{\%}2899{\%}2900075-8.pdf:pdf},
issn = {09586946},
journal = {International Dairy Journal},
number = {3-6},
pages = {281--286},
title = {{On the self-assembly of sodium caseinate}},
volume = {9},
year = {1999}
}
@article{Wiorkowski1988,
abstract = {This paper focuses on the general problem of forecasting the maximum value of a time series which by the nature of the data must approach an asymptotic value. Examples of such series include the growth of organisms, the concentration of a chemical reagent during a reaction occurring over time or the amount of a fossil fuel resource which has been discovered or produced as a function of time. The approach taken below differs from the usual models for this type of data in that it assumes that an unobserved time series is actually driving the process, and that the observed data series is a function of the unobserved process. In the case of fossil fuels the unobserved series might be a measure of the exploratory drilling, the number of production days in a given time period or even the amount of fiscal resources devoted to exploratory activities. A maximum likelihood method is developed for estimating the parameters of the process, especially the maximum S, and the covariance structure of the estimators is developed. The methodology is illustrated on an example of oil production. Finally, methods are developed for forecasting the data into the near future. Copyright {\textcopyright} 1988 John Wiley {\&} Sons, Ltd.},
author = {Wiorkowski, John J.},
doi = {10.1002/for.3980070406},
file = {:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/Wiorkowski - 1988 - Fitting of growth curves over time when the data are obtained from a single realization.pdf:pdf},
issn = {1099131X},
journal = {Journal of Forecasting},
keywords = {Asymptotic growth curves,Fossil fuel resources,Growth curves,Maximum likelihood estimation,Time series},
number = {4},
pages = {259--272},
title = {{Fitting of growth curves over time when the data are obtained from a single realization}},
volume = {7},
year = {1988}
}
