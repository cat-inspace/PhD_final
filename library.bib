isbn = {0021-9673},
issn = {00219673},
journal = {Journal of Chromatography A},
keywords = {Bovine milk protein,Casein,Genetic variants,HPLC,Polymorphisms,Whey protein},
number = {1-2},
pages = {101--106},
pmid = {18495141},
title = {{Validation of a new reversed-phase high-performance liquid chromatography method for separation and quantification of bovine milk protein genetic variants}},
volume = {1195},
