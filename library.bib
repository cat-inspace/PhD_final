@article{McCarthy2011,
abstract = {Rheology is the study of deformation and flow of matter. Rheological behavior of milk is, in a general sense, typical of a semidilute emulsion/suspension. However, the complex composition and the microstructure of milk result in complexities in behavior not exhibited by simple systems of this kind. These complexities become more pronounced when milk is subjected to such treatments as concentration (of fat or of total solids), fractionation, homogenization, heating, renneting, and acidification during its conversion to dairy products.In this article, quantitative descriptions (mathematical models) of the fundamental rheological properties of milk, cream, concentrated milk and cream, sweetened condensed milk, dulce de leche, ice cream mix, yogurt, buttermilk, and fresh cheeses are presented. The models are of three kinds: those that relate rheological behavior to composition and microstructure, those that quantify objective properties related to texture (texture itself must be measured subjectively), and phenomenological models useful for process engineering design and other technological purposes.It is shown that though there are some general similarities between the different milk products (e.g., time-dependent shear thinning and viscoelasticity are almost ubiquitous), sensitivity of rheological properties to composition and microstructure has necessitated the development of product-specific as well as general approaches to describing rheological behavior.},
author = {McCarthy, O. J.},
doi = {10.1016/B978-0-12-374407-4.00029-7},
file = {:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/McCarthy - 2011 - Rheology of Liquid and Semi-Solid Milk Products.pdf:pdf},
isbn = {9780123744029},
issn = {00189448},
journal = {Encyclopedia of Dairy Sciences: Second Edition},
keywords = {Buttermilk,Cream,Dulce de leche,Fresh cheese,Ice cream mix,Milk,Milk concentrates,Newtonian,Non-Newtonian,Rheology,Shear thinning,Sweetened condensed milk,Time dependency,Viscoelasticity,Viscosity,Yogurt},
pages = {520--531},
title = {{Rheology of Liquid and Semi-Solid Milk Products}},
volume = {c},
year = {2011}
}
@article{Ningtyas2017,
abstract = {Fat globules interspersed in the protein network have a major role in cream cheese texture that greatly impacts on its consumer acceptability. This study investigated the effects of fat content on the lubrication, rheological, and structural characteristics of cream cheeses manufactured with 0.5, 5.5 or 11.6 percent (w/w) fat content. All three cheese samples showed viscoelastic, non-Newtonian and shear thinning behaviour. The fat contents were shown to affect tribological behaviour and their high-speed regimes correlated well with the bulk rheology (viscosity). The low-fat content was associated with higher friction, firmer texture and reduced spreadability of the cream cheese, due to fewer fat globules being interspersed in the protein matrix, which was confirmed from confocal images.},
author = {Ningtyas, Dian Widya and Bhandari, Bhesh and Bansal, Nidhi and Prakash, Sangeeta},
