file = {:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/Matia-Merino, Lau, Dickinson - 2004 - Effects of low-methoxyl amidated pectin and ionic calcium on rheology and microstructure of acid-i.pdf:pdf},
