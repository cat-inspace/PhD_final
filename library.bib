@article{Guinee2016a,
abstract = {The rheology of cheese characterizes its deformation behavior when subjected to stress or strain. The rheological behavior of cheese can be measured by an array of tests. Some tests, for example creep and low-strain oscillation rheometry, involve application of strains within the linear viscoelastic limit and yield intrinsic rheological quantities such as storage modulus and elastic creep compliance. These tests are also useful in characterizing cheese as a viscoelastic material exhibiting the properties of ideal solids and liquids. Other tests employ strains outside the viscoelastic range such as large strain compression or torsion gelometry and yield quantities (e.g., fracture stress, fracture strain, and/or hardness) that are more empirical but which simulate the stresses and strains encountered during consumption and commercial size-reduction operations (including shredding and grinding). The rheological characteristics of cheese are controlled by its macrostructure, microstructure, composition, and internal environment (e.g., pH, temperature). The primary structural component controlling the deformation of cheese for a given stress is the protein matrix, with its concentration and degree of proteolysis and hydration being critical.},
author = {Guinee, T.P.},
doi = {10.1016/B978-0-08-100596-5.00663-6},
file = {:Users/catinspace/Library/Application Support/Mendeley Desktop/Downloaded/Guinee - 2016 - Cheese Cheese Rheology ☆.pdf:pdf},
