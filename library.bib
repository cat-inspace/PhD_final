isbn = {0958-6946},
issn = {09586946},
journal = {International Dairy Journal},
number = {12},
pages = {979--986},
title = {{The effect of combinations of sodium phosphates in binary mixtures on selected texture parameters of processed cheese spreads}},
volume = {21},
year = {2011}
}
